        <mxCell id="KOPt0NSH-wPXF6KEeLkA-22" value="11. Cloud Provisioner" style="swimlane;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;" vertex="1" parent="1">
